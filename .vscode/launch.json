{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Meteo: Build and Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/meteo/build/meteo.elf",
            "args": ["-c", "/home/locchan/dev/HW-Tinkering/meteo/cfg/config.cfg", "-d", "-o", "/tmp/offload_test"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "meteo_clean_and_build",
            "miDebuggerPath": "/usr/bin/gdb"
        },
    ]
}
